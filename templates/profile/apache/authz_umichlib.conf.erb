DBDParams <%= @dbd_params %>
# parameters: client_ip, client_host, my_userid, my_coll, authzd, authzd_coll, public_coll
# assumes in_addr_t is an unsigned int
# DBDPrepareSQL "SELECT * from table(authz_umichlib.authzd_to_coll_function(%u,%s,%s,%s))" authzd_stmt
DBDPrepareSQL "SELECT * from table(authz_umichlib.authzd_to_coll_function(%u,%s,%s,%s))" authzd_stmt
# parameters: dlpsServer, dlpsPath
# result columns: coll, dlpsPath, dlpsAuthenMethod, dlpsAuthzType
DBDPrepareSQL "SELECT coll, dlpsPath, dlpsAuthenMethod, dlpsAuthzType FROM aa_coll_obj, aa_coll WHERE dlpsServer = %s AND %s LIKE dlpsPath AND coll = uniqueIdentifier ORDER BY LENGTH(dlpsPath) DESC" protected_stmt
DBDriver oracle

DLPSAuthExemptionPolicy checkOnlyListed
DLPSAuthExemptionPaths <%= @exempt_paths %>
DLPSAuthFilenameHandlers application/x-httpd-php bwshare-info bwshare-trace cgi-script auth_openidc redirect-handler server-status drupal_security_do_not_remove_see_sa_2006_006 fcgid-script drupal_security_do_not_remove_see_sa_2013_003 shib-handler proxy:unix:/run/php/php8.1-fpm.sock|fcgi://localhost
DLPSAuthURIHandlers jakarta-servlet proxy-server
